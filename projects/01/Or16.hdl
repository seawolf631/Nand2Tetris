	Not(in=y7 , out=out[7]);
	Not(in=a[8], out=w8);
	Not(in=b[8], out=x8);    
	And(a=w8, b=x8, out=y8);
	Not(in=y8 , out=out[8]);
	Not(in=a[9], out=w9);
	Not(in=b[9], out=x9);    
	And(a=w9, b=x9, out=y9);
	Not(in=y9 , out=out[9]);
	Not(in=a[10], out=w10);
	Not(in=b[10], out=x10);    
	And(a=w10, b=x10, out=y10);
