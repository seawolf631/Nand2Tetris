	Not(in=y12 , out=out[12]);
	Not(in=a[13], out=w13);
	Not(in=b[13], out=x13);    
	And(a=w13, b=x13, out=y13);
	Not(in=y13 , out=out[13]);
	Not(in=a[14], out=w14);
	Not(in=b[14], out=x14);    
	And(a=w14, b=x14, out=y14);
	Not(in=y14 , out=out[14]);
	Not(in=a[15], out=w15);
