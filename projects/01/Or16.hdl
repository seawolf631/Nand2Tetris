/**
 * 16-bit bitwise Or:
 * for i = 0..15 out[i] = (a[i] or b[i])
 */

CHIP Or16 {
    IN a[16], b[16];
    OUT out[16];

    PARTS:
    // Put your code here:
	Not(in=a[0], out=w0);
	Not(in=b[0], out=x0);    
	And(a=w0, b=x0, out=y0);
	Not(in=y0 , out=out[0]);
	Not(in=a[1], out=w1);
	Not(in=b[1], out=x1);    
	And(a=w1, b=x1, out=y1);
	Not(in=y1 , out=out[1]);
	Not(in=a[2], out=w2);
	Not(in=b[2], out=x2);    
	And(a=w2, b=x2, out=y2);
	Not(in=y2 , out=out[2]);
	Not(in=a[3], out=w3);
	Not(in=b[3], out=x3);    
	And(a=w3, b=x3, out=y3);
	Not(in=y3 , out=out[3]);
	Not(in=a[4], out=w4);
	Not(in=b[4], out=x4);    
	And(a=w4, b=x4, out=y4);
	Not(in=y4 , out=out[4]);
	Not(in=a[5], out=w5);
	Not(in=b[5], out=x5);    
	And(a=w5, b=x5, out=y5);
	Not(in=y5 , out=out[5]);
	Not(in=a[6], out=w6);
	Not(in=b[6], out=x6);    
	And(a=w6, b=x6, out=y6);
	Not(in=y6 , out=out[6]);
	Not(in=a[7], out=w7);
	Not(in=b[7], out=x7);    
